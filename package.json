{
  "name": "fosscode",
  "version": "0.0.34",
  "description": "Lightweight CLI application for code agent interactions with LLMs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fosscode": "dist/index.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/yoga.wasm",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --external \"@anthropic-ai/sdk\" --external \"openai\" --external \"chalk\" --external \"ink\" --external \"react\" --external \"yoga-wasm-web\" --external \"puppeteer\" && cp node_modules/yoga-wasm-web/dist/yoga.wasm dist/",
    "build:exe": "bun build src/binary.ts --target node --compile --production --outfile dist/fosscode",
    "build:docker": "docker build -t fosscode .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts --quiet",
    "format": "prettier --write src/**/*.ts",
    "test": "bun test --testPathIgnorePatterns=\"e2e\"",
    "test:all": "bun test",
    "clean": "rm -rf dist",
    "start": "bun run src/index.ts",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major",
    "release": "./scripts/release.sh patch",
    "publish": "./scripts/publish.sh",
    "perf": "node scripts/performance-test.js",
    "performance-test": "node scripts/performance-test.js"
  },
  "keywords": [
    "cli",
    "llm",
    "ai",
    "code-agent",
    "typescript",
    "terminal"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "20.19.11",
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cli-testing-library": "^3.0.1",
    "eslint": "9.34.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "ink-testing-library": "4.0.0",
    "jest": "29.6.0",
    "jest-environment-jsdom": "29.6.0",
    "prettier": "3.6.2",
    "source-map": "^0.7.6",
    "ts-jest": "29.1.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.60.0",
    "@grammyjs/types": "^3.22.1",
    "@modelcontextprotocol/sdk": "1.17.4",
    "@playwright/mcp": "0.0.35",
    "@types/jsdom": "^21.1.7",
    "abort-controller": "^3.0.0",
    "ansi-styles": "^6.2.1",
    "array.prototype.findindex": "^2.2.4",
    "chalk": "5.6.0",
    "cheerio": "1.1.2",
    "commander": "11.1.0",
    "event-target-shim": "^6.0.2",
    "grammy": "^1.38.1",
    "ink": "4.4.1",
    "ink-spinner": "5.0.0",
    "ink-text-input": "5.0.1",
    "jsdom": "^26.1.0",
    "json-stable-stringify-without-jsonify": "^1.0.1",
    "openai": "4.104.0",
    "puppeteer": "^24.17.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "yoga-wasm-web": "0.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "react-devtools-core": "^4.19.1"
  },
  "peerDependenciesMeta": {
    "react-devtools-core": {
      "optional": true
    }
  },
  "preferUnplugged": true
}
