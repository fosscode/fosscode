{
  "name": "fosscode",
  "version": "0.0.112",
  "description": "Lightweight CLI application for code agent interactions with LLMs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fosscode": "dist/index.js"
  },
  "files": [
    "dist/index.js",
    "dist/yoga.wasm",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --external \"@anthropic-ai/sdk\" --external \"openai\" --external \"chalk\" --external \"ink\" --external \"react\" --external \"yoga-wasm-web\" && cp node_modules/yoga-wasm-web/dist/yoga.wasm dist/",
    "build:exe": "bun build src/binary.ts --target node --compile --outfile dist/fosscode",
    "build:docker": "docker build -t fosscode .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts --quiet",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:e2e": "jest --testPathPatterns=e2e",
    "test:all": "jest",
    "clean": "rm -rf dist",
    "postinstall": "node ./scripts/patch-vulnerable-deps.js",
    "start": "bun run src/index.ts",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major",
    "release": "./scripts/release.sh patch",
    "publish": "./scripts/publish.sh"
  },
  "keywords": [
    "cli",
    "llm",
    "ai",
    "code-agent",
    "typescript",
    "terminal"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "8",
    "ink-testing-library": "^4.0.0",
    "jest": "^30.1.1",
    "jest-circus": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testRunner": "jest-circus/runner",
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.ts"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx",
      "**/?(*.)+(spec|test).ts",
      "**/?(*.)+(spec|test).tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(ink|ink-testing-library|react|yoga-wasm-web|execa)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "moduleNameMapper": {
      "^(.{1,2}/.*).js$": "$1",
      "^ink$": "<rootDir>/src/__mocks__/ink.js",
      "^ink-spinner$": "<rootDir>/src/__mocks__/ink-spinner.js",
      "^ink-testing-library$": "<rootDir>/src/__mocks__/ink-testing-library.js"
    }
  },
