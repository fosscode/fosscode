{
  "defaultProvider": "openai",
  "defaultModel": "gpt-3.5-turbo",
  "maxConversations": 100,
  "theme": "dark",
  "showContextPercentage": true,
  "contextDisplayFormat": "both",
  "contextWarningThreshold": 80,
  "providers": {
    "openai": {
      "apiKey": "your-openai-api-key-here",
      "organization": "your-org-id-optional",
      "timeout": 30000,
      "maxRetries": 3,
      "model": "gpt-3.5-turbo",
      "verbose": false
    },
    "grok": {
      "apiKey": "your-grok-api-key-here",
      "baseURL": "https://api.x.ai/v1",
      "timeout": 30000,
      "maxRetries": 3,
      "model": "sonic-fast-1",
      "verbose": false
    },
    "lmstudio": {
      "baseURL": "http://localhost:1234",
      "timeout": 60000,
      "maxRetries": 2,
      "model": "local-model",
      "verbose": true
    },
    "openrouter": {
      "apiKey": "your-openrouter-api-key-here",
      "baseURL": "https://openrouter.ai/api/v1",
      "timeout": 30000,
      "maxRetries": 3,
      "model": "anthropic/claude-3-haiku",
      "verbose": false
    },
    "sonicfree": {
      "baseURL": "https://gateway.opencode.ai/v1",
      "timeout": 30000,
      "maxRetries": 3,
      "model": "sonic",
      "verbose": false
    },
    "mcp": {
      "mcpServerCommand": "npx",
      "mcpServerArgs": ["@playwright/mcp@latest"],
      "timeout": 30000,
      "verbose": true
    }
  },
